<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" metadata-complete="true" version="3.1">
  <description>Based on palestink framework</description>
  <display-name>palestink framework</display-name>
  <filter>
    <filter-name>transformFilter</filter-name>
    <filter-class>framework.filter.TransformFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>transformFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <listener>
    <listener-class>framework.core.LoadResource</listener-class>
  </listener>
</web-app>