<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
<config>
    <dispatch>
        <servlet>
            <name>getNameList</name>
            <description><![CDATA[获取模块名称列表]]></description>
            <namespace>
                <![CDATA[
                    [custom][user_security][module.antcolony.necessary.Custom.getNameList][];
                ]]>
            </namespace>
        </servlet>
        <servlet>
            <name>getPermissionList</name>
            <description><![CDATA[获取模块权限列表]]></description>
            <namespace>
                <![CDATA[
                    [custom][user_security][module.antcolony.necessary.Custom.getPermissionList][];
                ]]>
            </namespace>
        </servlet>
        <servlet>
            <name>getSqlContent</name>
            <description><![CDATA[获取模块的sql.xml文件内容]]></description>
            <namespace>
                <![CDATA[
                    [custom][user_security][module.antcolony.necessary.Custom.getSqlContent][name];
                ]]>
            </namespace>
            <parameters>
                <parameter>
                    <name>name</name>
                    <description><![CDATA[模块名称（数字、大小写字母、下划线、横线）]]></description>
                    <type>string</type>
                    <format>^[0-9a-zA-Z_\-]{1,}$</format>
                    <allowNull>false</allowNull>
                </parameter>
            </parameters>
        </servlet>
        <servlet>
            <name>downloadServerResourceFile</name>
            <description><![CDATA[下载服务器的资源文件]]></description>
            <namespace>
                <![CDATA[
                    [custom][user_security][module.antcolony.necessary.Custom.downloadServerResourceFile][name,type];
                ]]>
            </namespace>
            <parameters>
                <parameter>
                    <name>name</name>
                    <description><![CDATA[模块名称（数字、大小写字母、下划线、横线）]]></description>
                    <type>string</type>
                    <format>^[0-9a-zA-Z_\-]{1,}$</format>
                    <allowNull>false</allowNull>
                </parameter>
                <parameter>
                    <name>type</name>
                    <description><![CDATA[资源类型（sql或dispatch）（注意大小写）]]></description>
                    <type>string</type>
                    <format>^(sql|dispatch)$</format>
                    <allowNull>false</allowNull>
                </parameter>
            </parameters>
        </servlet>
    </dispatch>
</config>
