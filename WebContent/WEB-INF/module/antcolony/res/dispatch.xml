                    [custom][antcolony][module.antcolony.necessary.Custom.uploadServerResourceFile][attachment,moduleName,fileName];
                ]]>
            </namespace>
            <parameters>
                <parameter>
                    <name>attachment</name>
                    <description><![CDATA[文件（尺寸小于等于10M，类型范围：xml）]]></description>
                    <type>file</type>
                    <format>.+\.(xml)$</format>
                    <fileMaxSize>10485760</fileMaxSize>
